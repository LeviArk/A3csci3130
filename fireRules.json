{
  "rules": {
    ".read": "true",
    ".write": "true",
    "contacts": {
      "$contact": {
        "name": {
          ".validate": "newData.isString() && newData.val().length <= 48 && newData.val().length >= 2"
        },
        "BID": {
          ".validate": "newData.val().length == 9"
        },
        "Address":{
          ".validate": "newData.val().length<50"
        }
      }
    }
  }
}